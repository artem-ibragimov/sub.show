{
   "root": true,
   "parser": "@typescript-eslint/parser",
   "plugins": [
      "@typescript-eslint"
   ],
   "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
   ],
   "rules": {
      "@typescript-eslint/no-unused-vars": [
         "error", { "argsIgnorePattern": "^_" }
      ],
      "@typescript-eslint/no-inferrable-types": "0"
   }
}